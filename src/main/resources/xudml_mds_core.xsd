<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:obis="http://www.oracle.com/obis/repository"
  xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
  xmlns:md="http://xmlns.oracle.com/bali/xml/metadata" 
  xmlns:mds="http://xmlns.oracle.com/mds"
  xmlns:mdsrel="http://xmlns.oracle.com/mds"
  targetNamespace="http://www.oracle.com/obis/repository"
  xmlns:jxb="http://java.sun.com/xml/ns/jaxb" jxb:version="2.1" 
  elementFormDefault="qualified">


  <xs:annotation>
    <xs:appinfo>
      <jaxb:schemaBindings>
        <!-- JAXB 1.0 annotation that allows us to specify the package name -->
        <jaxb:package name="oracle.bi.mds.server.object"/>
      </jaxb:schemaBindings>
    </xs:appinfo>
  </xs:annotation>

  <!-- basic types -->
  <xs:simpleType name="nameIDTypeStr_t">
    <xs:restriction base="xs:string">
      <xs:maxLength value="5000"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="largeString_t">
    <xs:restriction base="xs:string">
      <xs:maxLength value="1000000"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="voidId_t">
    <xs:restriction base="obis:nameIDTypeStr_t">
      <xs:pattern value="[1-9]([0-9])*:([0-9])*"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="ref_t">
    <xs:attribute name="refId" type="obis:nameIDTypeStr_t" use="required"/>
  </xs:complexType>
  <xs:complexType name="object_t">
    <xs:sequence>
      <xs:element name="Description" type="obis:largeString_t" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="name" type="obis:nameIDTypeStr_t" use="required"/>
    <xs:attribute name="id" type="obis:voidId_t" use="optional"/>
    <xs:attribute name="iconIndex" type="xs:unsignedInt" default="0"/>
    <xs:attribute name="isDefaultReadable" type="xs:boolean" default="true"/>
    <xs:attribute name="isDefaultReadWriteable" type="xs:boolean" default="false"/>
    <xs:attribute name="isDefaultDeleteable" type="xs:boolean" default="false"/>
    <xs:attribute name="isDefaultGrantable" type="xs:boolean" default="false"/>
    <xs:attribute name="isDefaultAll" type="xs:boolean" default="false"/>
    <xs:attribute name="isBIExtenderObject" type="xs:boolean" default="false"/>
    <xs:attribute name="mdsid" type="xs:ID" use="required"/>
  </xs:complexType>  
  <xs:simpleType name="join_t">
    <xs:restriction base="obis:nameIDTypeStr_t">
      <xs:enumeration value="Inner"/>
      <xs:enumeration value="LeftOuter"/>
      <xs:enumeration value="RightOuter"/>
      <xs:enumeration value="FullOuter"/>
      <xs:enumeration value="FullOuterStitch"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="tags_t">
      <xs:attribute name="name" type="xs:string"/>
  </xs:complexType>
  <xs:complexType name="options_t">
    <xs:attribute name="name" type="xs:string"/>
    <xs:attribute name="value" type="xs:string"/>
  </xs:complexType>
  
  <xs:complexType name="ConfigParameter_t">
       <xs:sequence>
         <xs:element name="Value" type="obis:largeString_t" minOccurs="1" maxOccurs="1"/>
       </xs:sequence>
       <xs:attribute name="name" type="obis:nameIDTypeStr_t" use="optional"/>
  </xs:complexType>

  <xs:complexType name="refExpressionObject_t">
    <xs:complexContent>
      <xs:extension base="obis:ref_t">
        <xs:attribute name="objectTypeId" type="obis:nameIDTypeStr_t"/>
        <xs:attribute name="objectRef" type="obis:nameIDTypeStr_t" use="required">
          <xs:annotation>
            <xs:appinfo>
              <md:attributeMetadata>
                <mdsrel:toParentRole>ObjectInExpr</mdsrel:toParentRole>
                <mdsrel:parentType>(xmlns(obis=http://www.oracle.com/obis/repository))/obis:object_t</mdsrel:parentType>
                <mdsrel:cascadeDelete>true</mdsrel:cascadeDelete>
                <mdsrel:integrityOn>false</mdsrel:integrityOn>
                <mdsrel:multiplicity>many</mdsrel:multiplicity>
              </md:attributeMetadata>
            </xs:appinfo>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <xs:complexType name="refTableExpressionObject_t">
    <xs:sequence>
      <xs:element name="RefObject" type="obis:refExpressionObject_t" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  
  <!-- expression -->
  <xs:complexType name="expression_t">
    <xs:complexContent>
      <xs:extension base="obis:object_w">
        <xs:sequence>
          <xs:element name="ExprText" type="obis:largeString_t" minOccurs="0"/>
          <xs:element name="ExprTextDesc" type="obis:largeString_t" minOccurs="0"/>
          <xs:element name="ObjectRefList" type="obis:refTableExpressionObject_t" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  
  <!-- logical column -->
  <xs:complexType name="logicalColumn_t">
    <xs:complexContent>
      <xs:extension base="obis:object_w">
        <xs:sequence>
          <xs:element name="Expr" type="obis:expression_w" minOccurs="0"/>
          <xs:element ref="obis:MeasureDefn" minOccurs="0"/>
          <xs:element ref="obis:AttrDefn" minOccurs="0"/>
          <xs:element name="AttributeDefn" type="obis:expression_w" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="isDerived" type="xs:boolean" use="optional" default="false"/>
        <xs:attribute name="isWriteable" type="xs:boolean" use="optional" default="false"/>

        <!-- Reference to custom sort order column -->
        <xs:attribute name="customSortOrderRef" type="obis:nameIDTypeStr_t">
          <xs:annotation>
            <xs:appinfo>
              <md:attributeMetadata>
                <mdsrel:toParentRole>CustomSortOrder</mdsrel:toParentRole>
                <mdsrel:parentType>(xmlns(obis=http://www.oracle.com/obis/repository))/obis:logicalColumn_t</mdsrel:parentType>
                <mdsrel:cascadeDelete>false</mdsrel:cascadeDelete>
                <mdsrel:integrityOn>false</mdsrel:integrityOn>
                <mdsrel:multiplicity>many</mdsrel:multiplicity>
              </md:attributeMetadata>
            </xs:appinfo>
          </xs:annotation>
        </xs:attribute>

        <!-- Reference to descriptor id column -->
        <xs:attribute name="descriptorIDRef" type="obis:nameIDTypeStr_t">
          <xs:annotation>
            <xs:appinfo>
              <md:attributeMetadata>
                <mdsrel:toParentRole>DescriptorID</mdsrel:toParentRole>
                <mdsrel:parentType>(xmlns(obis=http://www.oracle.com/obis/repository))/obis:logicalColumn_t</mdsrel:parentType>
                <mdsrel:cascadeDelete>false</mdsrel:cascadeDelete>
                <mdsrel:integrityOn>false</mdsrel:integrityOn>
                <mdsrel:multiplicity>many</mdsrel:multiplicity>
              </md:attributeMetadata>
            </xs:appinfo>
          </xs:annotation>
        </xs:attribute>

      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- BusinessModel -->
  <xs:complexType name="refLogicalDisplayFolder_t">
    <xs:complexContent>
      <xs:extension base="obis:ref_t">
        <xs:attribute name="logicalDisplayFolderRef" type="obis:nameIDTypeStr_t" use="required">
          <xs:annotation>
            <xs:appinfo>
              <md:attributeMetadata>
                <mdsrel:toParentRole>LogicalDisplayFolder</mdsrel:toParentRole>
                <mdsrel:parentType>(xmlns(obis=http://www.oracle.com/obis/repository))/obis:logicalDisplayFolder_t</mdsrel:parentType>
                <mdsrel:cascadeDelete>true</mdsrel:cascadeDelete>
                <mdsrel:integrityOn>false</mdsrel:integrityOn>
                <mdsrel:multiplicity>many</mdsrel:multiplicity>
              </md:attributeMetadata>
            </xs:appinfo>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="refTableLogicalDisplayFolder_t">
    <xs:sequence>
      <xs:element name="RefLogicalDisplayFolder" type="obis:refLogicalDisplayFolder_t" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="businessModel_t">
    <xs:complexContent>
      <xs:extension base="obis:object_w">
        <xs:sequence>
          <xs:element name="RefDisplayFolders" type="obis:refTableLogicalDisplayFolder_t" minOccurs="0" maxOccurs="1"/> <!-- NOTE: required for storing child order -->
        </xs:sequence>
        <xs:attribute name="isClassicStar" type="xs:boolean"/>
        <xs:attribute name="isAvailable" type="xs:boolean"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- logicalcomplexjoin  -->
  <xs:complexType name="logicalComplexJoin_t">
    <xs:complexContent>
      <xs:extension base="obis:object_w">
        <xs:attribute name="type" type="obis:join_t"/>

        <!-- Reference to driving table -->
        <xs:attribute name="drivingTableRef" type="obis:nameIDTypeStr_t">
          <xs:annotation>
            <xs:appinfo>
              <md:attributeMetadata>
                <mdsrel:toParentRole>DrivingTable</mdsrel:toParentRole>
                <mdsrel:parentType>(xmlns(obis=http://www.oracle.com/obis/repository))/obis:logicalTable_t</mdsrel:parentType>
                <mdsrel:cascadeDelete>false</mdsrel:cascadeDelete>
                <mdsrel:integrityOn>false</mdsrel:integrityOn>
                <mdsrel:multiplicity>many</mdsrel:multiplicity>
              </md:attributeMetadata>
            </xs:appinfo>
          </xs:annotation>
        </xs:attribute>

        <!-- Reference to Logical Table 1 -->
        <xs:attribute name="logicalTable1Ref" type="obis:nameIDTypeStr_t" use="required">
          <xs:annotation>
            <xs:appinfo>
              <md:attributeMetadata>
                <mdsrel:toParentRole>LogicalTable1</mdsrel:toParentRole>
                <mdsrel:toChildRole>LogicalComplexJoinList1</mdsrel:toChildRole>
                <mdsrel:parentType>(xmlns(obis=http://www.oracle.com/obis/repository))/obis:logicalEntity_t</mdsrel:parentType>
                <mdsrel:cascadeDelete>true</mdsrel:cascadeDelete>
                <mdsrel:integrityOn>false</mdsrel:integrityOn>
                <mdsrel:multiplicity>many</mdsrel:multiplicity>
              </md:attributeMetadata>
            </xs:appinfo>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="multiplicity1" type="obis:nameIDTypeStr_t"/>
        <xs:attribute name="isAggregate1" type="xs:boolean"/>

        <!-- Reference to Logical Table 2 -->
        <xs:attribute name="logicalTable2Ref" type="obis:nameIDTypeStr_t" use="required">
          <xs:annotation>
            <xs:appinfo>
              <md:attributeMetadata>
                <mdsrel:toParentRole>LogicalTable2</mdsrel:toParentRole>
                <mdsrel:toChildRole>LogicalComplexJoinList2</mdsrel:toChildRole>
                <mdsrel:parentType>(xmlns(obis=http://www.oracle.com/obis/repository))/obis:logicalEntity_t</mdsrel:parentType>
                <mdsrel:cascadeDelete>true</mdsrel:cascadeDelete>
                <mdsrel:integrityOn>false</mdsrel:integrityOn>
                <mdsrel:multiplicity>many</mdsrel:multiplicity>
              </md:attributeMetadata>
            </xs:appinfo>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="multiplicity2" type="obis:nameIDTypeStr_t"/>
        <xs:attribute name="isAggregate2" type="xs:boolean"/>

      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- entity - base type of logicalLevel and logicalTable -->
  <xs:complexType name="logicalEntity_t">
    <xs:complexContent>
      <xs:extension base="obis:object_w">
         <!-- Reference to parent subject area -->
        <xs:attribute name="subjectAreaRef" type="obis:nameIDTypeStr_t">
          <xs:annotation>
            <xs:appinfo>
              <md:attributeMetadata>
                <mdsrel:toParentRole>SubjectArea</mdsrel:toParentRole>
                <mdsrel:toChildRole>LogicalEntities</mdsrel:toChildRole>
                <mdsrel:parentType>(xmlns(obis=http://www.oracle.com/obis/repository))/obis:businessModel_t</mdsrel:parentType>
                <mdsrel:cascadeDelete>true</mdsrel:cascadeDelete>
                <mdsrel:integrityOn>false</mdsrel:integrityOn>
                <mdsrel:multiplicity>many</mdsrel:multiplicity>
              </md:attributeMetadata>
            </xs:appinfo>
          </xs:annotation>
        </xs:attribute>        
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- logical level -->
  <xs:complexType name="refChildLevel_t">
    <xs:complexContent>
      <xs:extension base="obis:ref_t">
        <xs:attribute name="logicalLevelRef" type="obis:nameIDTypeStr_t" use="required">
          <xs:annotation>
            <xs:appinfo>
              <md:attributeMetadata>
                <mdsrel:toParentRole>ChildLevel</mdsrel:toParentRole>
                <mdsrel:toChildRole>RefParentLevels</mdsrel:toChildRole>
                <mdsrel:parentType>(xmlns(obis=http://www.oracle.com/obis/repository))/obis:logicalLevel_t</mdsrel:parentType>
                <mdsrel:cascadeDelete>true</mdsrel:cascadeDelete>
                <mdsrel:integrityOn>false</mdsrel:integrityOn>
                <mdsrel:multiplicity>many</mdsrel:multiplicity>
              </md:attributeMetadata>
            </xs:appinfo>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="refTableChildLevel_t">
    <xs:sequence>
      <xs:element name="RefLogicalLevel" type="obis:refChildLevel_t" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="refPreferredDrillDown_t">
    <xs:complexContent>
      <xs:extension base="obis:ref_t">
        <xs:attribute name="logicalLevelRef" type="obis:nameIDTypeStr_t" use="required">
          <xs:annotation>
            <xs:appinfo>
              <md:attributeMetadata>
                <mdsrel:toParentRole>PreferredDrillDown</mdsrel:toParentRole>
                <mdsrel:toChildRole>RefPreferredDrillUp</mdsrel:toChildRole>
                <mdsrel:parentType>(xmlns(obis=http://www.oracle.com/obis/repository))/obis:logicalLevel_t</mdsrel:parentType>
                <mdsrel:cascadeDelete>true</mdsrel:cascadeDelete>
                <mdsrel:integrityOn>false</mdsrel:integrityOn>
                <mdsrel:multiplicity>many</mdsrel:multiplicity>
              </md:attributeMetadata>
            </xs:appinfo>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="refTablePreferredDrillDown_t">
    <xs:sequence>
      <xs:element name="RefLogicalLevel" type="obis:refPreferredDrillDown_t" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="refLogicalLevelColumn_t">
    <xs:complexContent>
      <xs:extension base="obis:ref_t">
        <xs:attribute name="logicalColumnRef" type="obis:nameIDTypeStr_t" use="required">
          <xs:annotation>
            <xs:appinfo>
              <md:attributeMetadata>
                <mdsrel:toParentRole>LogicalColumn</mdsrel:toParentRole>
                <mdsrel:toChildRole>RefLogicalLevels</mdsrel:toChildRole>
                <mdsrel:parentType>(xmlns(obis=http://www.oracle.com/obis/repository))/obis:logicalColumn_t</mdsrel:parentType>
                <mdsrel:cascadeDelete>true</mdsrel:cascadeDelete>
                <mdsrel:integrityOn>false</mdsrel:integrityOn>
                <mdsrel:multiplicity>many</mdsrel:multiplicity>
              </md:attributeMetadata>
            </xs:appinfo>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="refTableLogicalLevelColumn_t">
    <xs:sequence>
      <xs:element name="RefLogicalColumn" type="obis:refLogicalLevelColumn_t" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="relativeSeqNum_t">
    <xs:sequence>
      <xs:element name="RefLogicalLevel" type="obis:refChildLevel_t" minOccurs="1"/>
      <xs:element name="RefLogicalColumn" type="obis:refLogicalLevelColumn_t" minOccurs="1"/>
    </xs:sequence>
    <xs:attribute name="name" type="obis:nameIDTypeStr_t"/>
  </xs:complexType>

  <xs:complexType name="logicalLevel_t">
    <xs:complexContent>
      <xs:extension base="obis:logicalEntity_w">
        <xs:sequence>
          <xs:element ref="obis:LogicalKey" minOccurs="0" maxOccurs="unbounded"/> <!-- NOTE: Nest keys under level -->
          <xs:element name="RefChildLevels" type="obis:refTableChildLevel_t" minOccurs="0" maxOccurs="1"/>
          <xs:element name="RefPreferredDrillDown" type="obis:refTablePreferredDrillDown_t" minOccurs="0" maxOccurs="1"/>
          <xs:element name="RefLogicalColumns" type="obis:refTableLogicalLevelColumn_t" minOccurs="0" maxOccurs="1"/>
		      <xs:element name="AbsoluteSequenceNumber" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="RefLogicalColumn" type="obis:refLogicalLevelColumn_t" minOccurs="0"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
		      <xs:element name="RelativeSequenceNumbers" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="RelativeSequenceNumber" type="obis:relativeSeqNum_t" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>

        <xs:attribute name="isUniqueSourceKey" type="xs:boolean" use="optional"/>
        <xs:attribute name="isGTA" type="xs:boolean" use="optional"/>
        <xs:attribute name="isFDUC" type="xs:boolean" use="optional"/>
        <xs:attribute name="levelConst" type="obis:nameIDTypeStr_t" use="optional"/>
        <xs:attribute name="memberCount" type="xs:unsignedInt" use="optional"/>

      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- dimension -->
  <xs:complexType name="dimension_t">
    <xs:complexContent>
      <xs:extension base="obis:object_w">
        <xs:sequence>
          <xs:element ref="obis:LogicalLevel" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="isTimeDim" type="xs:boolean" default="false"/>
        <xs:attribute name="isRagged" type="xs:boolean" use="optional"/>
        <xs:attribute name="isSkipped" type="xs:boolean" use="optional"/>
        <xs:attribute name="isValueBased" type="xs:boolean" use="optional"/>

        <!-- Reference to default root level -->
        <xs:attribute name="defaultRootLevelRef" type="obis:nameIDTypeStr_t">
          <xs:annotation>
            <xs:appinfo>
              <md:attributeMetadata>
                <mdsrel:toParentRole>DefaultRootLevel</mdsrel:toParentRole>
                <mdsrel:parentType>(xmlns(obis=http://www.oracle.com/obis/repository))/obis:logicalLevel_t</mdsrel:parentType>
                <mdsrel:cascadeDelete>false</mdsrel:cascadeDelete>
                <mdsrel:integrityOn>false</mdsrel:integrityOn>
                <mdsrel:multiplicity>one</mdsrel:multiplicity>
              </md:attributeMetadata>
            </xs:appinfo>
          </xs:annotation>
        </xs:attribute>

        <!-- Reference to parent subject area -->
        <xs:attribute name="subjectAreaRef" type="obis:nameIDTypeStr_t">
          <xs:annotation>
            <xs:appinfo>
              <md:attributeMetadata>
                <mdsrel:toParentRole>SubjectArea</mdsrel:toParentRole>
                <mdsrel:toChildRole>LogicalDimensions</mdsrel:toChildRole>
                <mdsrel:parentType>(xmlns(obis=http://www.oracle.com/obis/repository))/obis:businessModel_t</mdsrel:parentType>
                <mdsrel:cascadeDelete>true</mdsrel:cascadeDelete>
                <mdsrel:integrityOn>false</mdsrel:integrityOn>
                <mdsrel:multiplicity>many</mdsrel:multiplicity>
              </md:attributeMetadata>
            </xs:appinfo>
          </xs:annotation>
        </xs:attribute>

      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- table of references to child LTS objects -->
  <xs:complexType name="refTableLogicalTableSource_t">
    <xs:sequence>
      <xs:element name="RefLogicalTableSource" type="obis:refLogicalTableSource_t" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <!-- logical table -->
  <xs:complexType name="logicalTable_t">
    <xs:complexContent>
      <xs:extension base="obis:logicalEntity_w">
        <xs:sequence>
          <xs:element ref="obis:LogicalColumn" minOccurs="0" maxOccurs="unbounded"/> <!-- NOTE: Nest columns under table -->
          <xs:element ref="obis:LogicalKey" minOccurs="0" maxOccurs="unbounded"/> <!-- NOTE: Nest keys under table -->
          <xs:element ref="obis:LogicalForeignKey" minOccurs="0" maxOccurs="unbounded"/> <!-- NOTE: Nest foreign keys under table -->
          <xs:element name="RefTableSources" type="obis:refTableLogicalTableSource_t" minOccurs="0" maxOccurs="1"/> <!-- NOTE: required for storing child order -->
        </xs:sequence>
        <xs:attribute name="isLookupTable" type="xs:boolean" use="optional"/>
        <xs:attribute name="x" type="xs:int" use="optional"/>
        <xs:attribute name="y" type="xs:int" use="optional"/>

        <!-- Reference to classic star level column -->
        <xs:attribute name="levelColumnRef" type="obis:nameIDTypeStr_t">
          <xs:annotation>
            <xs:appinfo>
              <md:attributeMetadata>
                <mdsrel:toParentRole>levelColumn</mdsrel:toParentRole>
                <mdsrel:parentType>(xmlns(obis=http://www.oracle.com/obis/repository))/obis:logicalColumn_t</mdsrel:parentType>
                <mdsrel:cascadeDelete>false</mdsrel:cascadeDelete>
                <mdsrel:integrityOn>false</mdsrel:integrityOn>
                <mdsrel:multiplicity>one</mdsrel:multiplicity>
              </md:attributeMetadata>
            </xs:appinfo>
          </xs:annotation>
        </xs:attribute>

      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- logical foreign key -->
  <xs:complexType name="refLogicalForeignKeyColumn_t">
    <xs:complexContent>
      <xs:extension base="obis:ref_t">
        <xs:attribute name="logicalColumnRef" type="obis:nameIDTypeStr_t" use="required">
          <xs:annotation>
            <xs:appinfo>
              <md:attributeMetadata>
                <mdsrel:toParentRole>LogicalColumn</mdsrel:toParentRole>
                <mdsrel:toChildRole>RefLogicalForeignKeys</mdsrel:toChildRole>
                <mdsrel:parentType>(xmlns(obis=http://www.oracle.com/obis/repository))/obis:logicalColumn_t</mdsrel:parentType>
                <mdsrel:cascadeDelete>true</mdsrel:cascadeDelete>
                <mdsrel:integrityOn>false</mdsrel:integrityOn>
                <mdsrel:multiplicity>many</mdsrel:multiplicity>
              </md:attributeMetadata>
            </xs:appinfo>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="refTableLogicalForeignKeyColumn_t">
    <xs:sequence>
      <xs:element name="RefLogicalColumn" type="obis:refLogicalForeignKeyColumn_t" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="logicalForeignKey_t">
    <xs:complexContent>
      <xs:extension base="obis:object_w">
        <xs:sequence>
          <xs:element name="RefColumns" type="obis:refTableLogicalForeignKeyColumn_t" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>

        <xs:attribute name="type" type="obis:join_t"/>

        <!-- Reference to logical key -->
        <xs:attribute name="counterPartKeyRef" type="obis:nameIDTypeStr_t">
          <xs:annotation>
            <xs:appinfo>
              <md:attributeMetadata>
                <mdsrel:toParentRole>CounterPartKey</mdsrel:toParentRole>
